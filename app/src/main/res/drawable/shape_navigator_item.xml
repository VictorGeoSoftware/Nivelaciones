<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="#330B0B61" />
            <corners
                android:radius="10dip" />
            <padding
                android:left="10dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#FF120A2A"/>
            <corners
                android:radius="10dip" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
            <stroke 
                android:width="1dip"
                android:color="#FFFFFFFF"/>
        </shape>
    </item>
</selector>
